{
    "name": "tyamahori/monolog-ltsv-formatter",
    "description": "LTSV Formatter for Monolog",
    "license": "MIT",
    "authors": [
        {
            "name": "tyamahori(Kazuki Tamahori)",
            "email": "tyamahori@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Tyamahori\\Monolog\\Formatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.0|^10.0",
        "phpstan/phpstan": "^1.9",
        "friendsofphp/php-cs-fixer": "^3.14"
    }
}
